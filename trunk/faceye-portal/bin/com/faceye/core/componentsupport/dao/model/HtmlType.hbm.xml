<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.faceye.core.componentsupport.dao.model.HtmlType" table="SYS_HTML_TYPE">
        <id name="id" unsaved-value="" type="java.lang.String">
          <column name="id" length="32"></column>
          <generator class="uuid.hex"></generator>
        </id>
        <property name="name" type="string">
          <column name="name" length="50"></column>
        </property>
       <property name="type" type="string">
        <column name="type" length="255"></column>
       </property>
       <property name="html" type="string">
        <column name="html" length="255"></column>
       </property>
      
	</class>
</hibernate-mapping>
