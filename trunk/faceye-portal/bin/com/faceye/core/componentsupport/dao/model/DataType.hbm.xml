<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.faceye.core.componentsupport.dao.model.DataType" table="SYS_DATA_TYPE">
        <id name="id" unsaved-value="null" type="java.lang.String">
          <column name="id" length="32"></column>
          <generator class="uuid.hex"></generator>
        </id>
        <property name="name" type="string">
          <column name="name" length="50"></column>
        </property>
       <property name="typeName" type="string">
        <column name="typeName" length="255"></column>
       </property>
       <property name="typeNumInSql" type="integer">
        <column name="typeNumInSql" length="255"></column>
       </property>
       <property name="typeOfClass" type="string">
        <column name="typeOfClass" length="100"></column>
       </property>
       
	</class>
</hibernate-mapping>
