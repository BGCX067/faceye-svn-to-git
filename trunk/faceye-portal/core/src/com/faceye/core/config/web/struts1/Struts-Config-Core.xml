	<global-forwards>
		<!--forward className="" contextRelative="true" id="idvalue24" module="" name="" path="" redirect="true">
			<icon id="idvalue25">
			<small-icon id="idvalue26">small-icon</small-icon>
			<large-icon id="idvalue27">large-icon</large-icon>
